# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET           /                           controllers.Application.index()


GET           /userProfile/:userId        controllers.BusinessUserProfileController.getProfile(userId)
POST          /userProfile/:userId        controllers.BusinessUserProfileController.updateProfile(userId)
DELETE        /userProfile/:userId        controllers.BusinessUserProfileController.deleteProfile(userId)
PUT           /userProfile/:userId        controllers.BusinessUserProfileController.updateProfile(userId)

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file               controllers.Assets.at(path="/public", file)
