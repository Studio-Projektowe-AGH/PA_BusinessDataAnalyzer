# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                                    controllers.Application.index()

# Accessing business user profile
GET        /business_stats/all/:userId        controllers.BusinessStatsController.getAll(userId)
POST       /business_stats/value/:userId        controllers.BusinessStatsController.getValue(userId)
POST       /business_stats/ratio/:userId        controllers.BusinessStatsController.getRatio(userId)


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                        controllers.Assets.at(path="/public", file)
