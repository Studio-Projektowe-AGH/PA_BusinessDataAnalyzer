# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET         /                     controllers.Application.index()

POST        /createProfile         controllers.BusinessUserProfileController.createProfile()
GET         /getProfile            controllers.BusinessUserProfileController.getBusinessUserProfile()
GET         /test                 controllers.TestOfWebServiceClient.restEasyTest()

POST        /updateProfile        controllers.BusinessUserProfileController.updateProfile()
# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file         controllers.Assets.at(path="/public", file)
